class tp2Menu extends Program{
    void algorithm(){

	int x;
	print("Bienvenu dans la matrice");
	println("1. ourvrir doc");
	println("2. cree doc");
	println("3. enregistrer doc");
	println("4. quitter");
	println("que voulez vous faire? ");
	x = readInt();
	if ( x == 1 )
	    println("vous voulez ouvrir");
	else if ( x == 2 )
	    println("vous voulez creer doc");
	else if ( x == 3 )
	    println("vous voulez enregistrer");
	else
	    println("vous voulez quitter");
    }
}
