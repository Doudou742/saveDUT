import tps.Carte;
public class EssaiCarte{
    public static void main(String[] arg){
	Carte[] paquet = new Carte[32];
	for (int i = 0; i < 4; i++){
	    for (int j = 0; j < 8; j++){
		paquet[i] = new Carte(i,j);
	    }
	}
	for(int i = 0; i < paquet.length; i++){
	    System.out.println(paquet[i].toString());
	}
    }
}
