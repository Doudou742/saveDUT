package fr.ulille.iut;

import java.util.Map;

public class CommandePizza {

	private String client;
	private int id;
	private int prix;
	private String pizzas;
	
	
	public CommandePizza() {
		
	}
	
	public CommandePizza(String client, int id, int prix, String pizzas) {
		super();
		this.client = client;
		this.id = id;
		this.prix = prix;
		this.pizzas = pizzas;
	}

	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getClient() {
		return client;
	}
	public void setClient(String client) {
		this.client = client;
	}
	public int getPrix() {
		return prix;
	}
	public void setPrix(int prix) {
		this.prix = prix;
	}

	public String getPizzas() {
		return pizzas;
	}

	public void setPizzas(String pizzas) {
		this.pizzas = pizzas;
	}

	

	
}
